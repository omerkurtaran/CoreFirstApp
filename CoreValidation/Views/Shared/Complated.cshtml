@model Register
@{
    Layout = "_Layout";
    ViewData["Title"] = "Complated";
}

<div class="bg-primary text-white m-1 p-1">
    <h2>Register Complate</h2>
</div>

<table class="table table-striped">
    <tr>
        <th>UserName</th>
        <td>@Model.UserName</td>
    </tr>
    <tr>
        <th>EMail</th>
        <td>@Model.Mail</td>
    </tr>

    <tr>
        <th>BirthDate</th>
        <td>@Model.BirthDate</td>
    </tr>
</table>

